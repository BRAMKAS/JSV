{
	"$schema" : "http://json-schema.org/hyper-schema#",
	"id" : "http://json-schema.org/hyper-schema#",

	"properties" : {
		"links" : {
			"type" : "array",
			"items" : {"$ref" : "http://json-schema.org/links#"},
			"optional" : true
		}
	},
	
	"links" : [
		{
			"href" : "{$ref}",
			"rel" : "full"
		},
		
		{
			"href" : "{$schema}",
			"rel" : "definedby"
		},
		
		{
			"href" : "{id}",
			"rel" : "self"
		}
	],
	
	"fragmentResolution" : "dot-delimited",
	"extends" : {"$ref" : "http://json-schema.org/schema#"}
}